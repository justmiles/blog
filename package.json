{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.2.2"
  },
  "scripts": {
    "start": "node_modules/hexo/bin/hexo serve -p 8080",
    "test": "ls",
    "build": "node_modules/hexo/bin/hexo clean && node_modules/hexo/bin/hexo generate",
    "watch": "node_modules/.bin/watch -p 'source/*/*.md' -c 'node_modules/.bin/hexo generate'",
    "serve": "node_modules/.bin/http-server public"
  },
  "dependencies": {
    "hexo": "^3.1.0",
    "hexo-generator-archive": "^0.1.2",
    "hexo-generator-category": "^0.1.2",
    "hexo-generator-feed": "^1.0.3",
    "hexo-generator-index": "^0.1.2",
    "hexo-generator-json-content": "^3.0.1",
    "hexo-generator-tag": "^0.1.1",
    "hexo-migrator-wordpress": "^0.1.2",
    "hexo-renderer-ejs": "^0.1.0",
    "hexo-renderer-marked": "^0.2.4",
    "hexo-renderer-scss": "^1.0.2",
    "hexo-renderer-stylus": "^0.3.0",
    "hexo-server": "^0.1.2"
  },
  "devDependencies": {
    "http-server": "^0.9.0",
    "watch-cli": "^0.2.1"
  }
}
